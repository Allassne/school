  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>Bamba_Ezigba</DataSourceName>
        <CommandText>SELECT p.desc_produit, SUM(p.cout_produit * v.quantite) AS vente_Total
FROM Client AS c
JOIN Vente AS v ON (c.clientID = v.clientID)
JOIN Produit AS p ON (p.produitID = v.produitID)
GROUP BY p.desc_produit</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
